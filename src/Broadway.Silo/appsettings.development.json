{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3}] {Message:j} <- {SourceContext} on thread {ThreadId}{NewLine}{Exception}"
        }
      }
    ]
  }
}
